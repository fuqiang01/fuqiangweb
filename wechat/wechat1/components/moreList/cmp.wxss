.container{
    display: flex;
    flex-wrap: wrap;
}
.item{
    width: 25%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 20rpx 0;
}
.item > .img{
    width: 60rpx;
}
.item > .text{
    padding-top: 10rpx;
    font-size: 30rpx;
    color: #777;
}