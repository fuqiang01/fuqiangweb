page{
    background-color: rgb(35,69,193);
}
.banner{
    background-image: url('http://demo.cwgszc.com/fuqiangwxminimg/bg_banner.png');
    background-size: 100% 100%;
    padding-top: 350rpx;
}
.attention-box{
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-bottom: 50rpx;
}
.attention-box f-h4{
    color: #fff;
}
.attention-box .scroll-box{
    background-color: #fff;
    box-sizing: border-box;
    width: 700rpx;
    height: 500rpx;
    padding: 20rpx;
    border-radius: 20rpx;
    display: flex;
    flex-direction: column;
}
.attention-box .scroll-box text{
    display: block;
    font-size: 35rpx;
    color: #555;
    margin: 20rpx 0;
}