/* banner */
.banner{
    width: 750rpx;
}
.banner > image{
    width: 100%;
}
/* content */
.content{
    background-color: #eee;
}
/* 企业核名 */
.nuclear{
    background-color: #fff;
    margin-bottom: 20rpx;
}
/* 地址注册 */
.registeredAddress{
    display: flex;
    background-color: #fff;
}
.registeredAddress > view{
    padding: 20rpx;
}
.registeredAddress > view .noAddress{
    display: block;
    padding-top: 13rpx;
}
.registeredAddress > view > text{
    display: block;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
    background: linear-gradient(to right,#065dff, #48a3fb);
    padding: 15rpx 0;
    border-radius: 20rpx;
}
/* 相关政策 */
.policy{
    background-color: #fff;
}
/* 前期准备材料 */
.prepare{
    background-color: #fff;
}
.prepare .pre-item{
    position: relative;
    padding: 20rpx;
    box-sizing: border-box;
}
.prepare .pre-item .item-text{
    position: absolute;
    padding: 10rpx 20rpx;
    box-sizing: border-box;
}
.item-text > .item-title{
    display: block;
    color: #777;
    margin: 0 0 10rpx;
}
.item-text > .item-info{
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}
.item-text > .item-info > view{
    display: flex;
    align-items: center;
    width: 100%;
}
.pre-item:nth-of-type(1) .item-text > .item-info > view{
    width: 50%;
}
.item-text > .item-info > view > image{
    width: 15rpx;
    margin-right: 20rpx;
}
.item-text > .item-info > view > text{
    font-size: 30rpx;
}
.pre-item:nth-of-type(1) .item-text > .item-info > view > text{
    color: #ca8d26;
}
.pre-item:nth-of-type(2) .item-text > .item-info > view > text{
    color: #ff670f;
}
.pre-item:nth-of-type(3) .item-text > .item-info > view > text{
    color: #708edb;
}
.prepare .pre-item > image{
    width: 100%;
}
/* 后期拿到的资料 */
.take{
    background-color: #fff;
}
scroll-view{
    width: 100%;
    white-space:nowrap
}
scroll-view .take-item {
    display: inline-block;
    width: 200rpx;
    text-align: center;
    background-color: #ebf2ff;
    margin: 0 30rpx;
    padding: 10rpx 30rpx;
}
scroll-view .take-item .img-wrapper{
    width: 100%;
    height: 300rpx;
    display: flex;
    align-items: center;
}
scroll-view .take-item .img-wrapper > image{
    width: 100%;
}
scroll-view .take-item > text{
    font-size: 30rpx;
    color: #777;
}
/* 公司注册流程 */
.process{
    background-color: #fff;
}
.process > view{
    padding: 0 10rpx;
    box-sizing: border-box;
}
.process-bot,
.process-top{
    display: flex;
}
.process-bot > view,
.process-top > view{
    width: 25%;
}
.process-bot view text,
.process-top view text{
    font-size: 28rpx;
    display: block;
    text-align: center;
}
.process-bot view .text-service,
.process-top view .text-service{
    color: #fff;
    padding: 10rpx 0;
    border-radius: 25rpx;
    position: relative;
}
.process-top view .text-service::after{
    content: '';
    border: 20rpx solid transparent;
    position: absolute;
    top: 55rpx;
    left: 75rpx;
}
.process-bot view .text-service::after{
    content: '';
    border: 20rpx solid transparent;
    position: absolute;
    top: -35rpx;
    left: 75rpx;
}
.process-index{
    height: 10rpx;
    background-color: #dceeff;
    display: flex;
    margin: 40rpx 0;
}
.process-index > view{
    width: 25%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.process-index > view > text{
    background-color: #a2d2ff;
    display: block;
    width: 50rpx;
    height: 50rpx;
    text-align: center;
    border-radius: 50%;
    color: #fff;
}
.colorOne .text-time{
    color: #5cda6b;
}
.colorOne .text-service{
    background-color: #5cda6b;
}
.colorOne .text-service::after{
    border-bottom-color:  #5cda6b!important;
}
.colorTwo .text-time{
    color: #ff9b3d;
}
.colorTwo .text-service{
    background-color: #ff9b3d;
}
.colorTwo .text-service::after{
    border-top-color:  #ff9b3d!important;
}
.colorThree .text-time{
    color: #44e5cd;
}
.colorThree .text-service{
    background-color: #44e5cd;
}
.colorThree .text-service::after{
    border-bottom-color:  #44e5cd!important;
}
.colorFore .text-time{
    color: #33c9f9;
}
.colorFore .text-service{
    background-color: #33c9f9;
}
.colorFore .text-service::after{
    border-top-color:  #33c9f9!important;
}
/* 优势 */
.advantage{
    background-color: #fff;
    padding-bottom: 30rpx;
}
.advantage .scroll{
    width: 100%;
    white-space: nowrap;
}
.advantage .scroll .advantage-item {
    display: inline-block;
    width: 200rpx;
    white-space: pre-wrap;
    margin: 0 15rpx;
    border-radius: 20rpx;
    padding: 15rpx;
    color: #fff;
}
.advantage .scroll .advantage-item:nth-of-type(1){
    background-color: #7868ff;
}
.advantage .scroll .advantage-item:nth-of-type(2){
    background-color: #ff9b3d;
}
.advantage .scroll .advantage-item:nth-of-type(3){
    background-color: #61bdfc;
}
.advantage .scroll .advantage-item:nth-of-type(4){
    background-color: #23d795;
}
.advantage .scroll .advantage-item:nth-of-type(5){
    background-color: #619bfc;
}
.advantage .scroll .advantage-item:nth-of-type(6){
    background-color: #6fc948;
}
.advantage .scroll .advantage-item .title{
    display: flex;
    justify-content: center;
    align-items: center;
}
.advantage .scroll .advantage-item .title image{
    margin-right: 25rpx;
    width: 35rpx;
}
.advantage .scroll .advantage-item > text{
    font-size: 30rpx;
}


/* 我们还能做更多 */
.more{
    background-color: #fff;
}